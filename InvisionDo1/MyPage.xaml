		xmlns:local="clr-namespace:dnvXamarinForms;assembly=dnvXamarinForms" 
		xmlns:eff="clr-namespace:dnvXamarinForms.Effects;assembly=dnvXamarinForms" 
		x:Class="InvisionDo1.MyPage" BackgroundColor="#F1F1F1" Padding="0" >
